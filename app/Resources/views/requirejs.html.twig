{% set assetsPath = app.debug ? "assets" : "assets-built" %}

<script src="{{ asset(assetsPath ~ '/vendor/requirejs/require.js') }}" data-main="{{ assetsPath }}/js/common.js"></script>

<script>
    requirejs.config({
        baseUrl: '/{{ assetsPath }}/js',
    });

    require(['common'], function (common) {
        require(['{{ module }}']);
    });
</script>
